#!/bin/sh

set -e

case "$1" in
    (install)
    @MEMO_SUB_PREFIX@/bin/launchctl load -w @MEMO_PREFIX@@MEMO_SUB_PREFIX@/Library/LaunchDaemons/com.nukesor.pueued.plist
    ;;
	(upgrade)
	@MEMO_SUB_PREFIX@/bin/launchctl unload @MEMO_PREFIX@@MEMO_SUB_PREFIX@/Library/LaunchDaemons/com.nukesor.pueued.plist
    @MEMO_SUB_PREFIX@/bin/launchctl load -w @MEMO_PREFIX@@MEMO_SUB_PREFIX@/Library/LaunchDaemons/com.nukesor.pueued.plist
	;;
	(*)
	exit 0
	;;
esac

exit 0
