<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.mikebrady.shairport-sync</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/sh</string>
		<string>@MEMO_PREFIX@@MEMO_SUB_PREFIX@/bin/shairport-sync-wrapper</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>UserName</key>
	<string>shairport-sync</string>
</dict>
</plist>
